{
  "/nestjsws/*": {
    "target": "ws://localhost:8080/",
    "secure": false,
    "ws": true,
    "pathRewrite": {
      "^/nestjsws": ""
    }
  },
  "/nestjs/*": {
    "target": "http://localhost:7001/api/v1",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/nestjs": ""
    }
  },
  "/golangws/*": {
    "target": "ws://localhost:7002/ws",
    "secure": false,
    "ws": true,
    "pathRewrite": {
      "^/golangws": ""
    }
  },
  "/golang/*": {
    "target": "http://localhost:7002/api/v1",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/golang": ""
    }
  }
}
